<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >



   
<Button android:id="@+id/localRunButton" 
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content" 
	android:text="Ejecuta --> Openssl version">
	</Button>


<TextView  
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:id="@+id/urlLabel" android:text="Parametros Opensl:"/>

<LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal">
	<EditText android:id="@+id/urlEdit" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_weight="1" android:text=""/>
	<Button android:id="@+id/remoteRunButton" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Ejecutar"></Button>
</LinearLayout>

<TextView android:id="@+id/outputView" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>

</LinearLayout>
